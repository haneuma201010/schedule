
<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <% if admin_logged_in? %>
          <li><%= link_to "ユーザー一覧", '#' %></li>
          <li><%= link_to "Profile", current_admin_user %></li>
          <li><%= link_to "プロフィール編集", edit_admin_user_path(current_admin_user) %></li>
          <li><%= link_to "Log out", admin_logout_path, method: :delete %></li>
              
        <% else %>
          <li><%= link_to "Log in", admin_login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>
